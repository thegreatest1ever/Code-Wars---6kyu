function estSubsets(arr) {
  let set = new Set(arr);
  let n = set.size;
  return 2 ** n - 1;
}
//
def est_subsets(arr):
    unique_elements = set(arr)
    n = len(unique_elements)
    return 2 ** n - 1
//
export function estSubsets<T>(list: T[]): number {
  const uniqueElements = new Set(list);
  const n = uniqueElements.size;
  return 2 ** n - 1;
}
